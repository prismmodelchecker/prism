mdp

module EightOut_allLoPositive
  // 0 initial; 1..5 to goal; 6..8 to sink; 9 goal; 10 sink
  s : [0..10] init 0;

  // --- action a ---
  // lo(goal)=5*0.02=0.10; hi(sink)=3*0.25=0.75 → Pmin(goal) ≥ max(0.10, 0.25)=0.25
  [a] s=0 ->
    [0.02,0.20] : (s'=1) +
    [0.02,0.20] : (s'=2) +
    [0.02,0.20] : (s'=3) +
    [0.02,0.20] : (s'=4) +
    [0.02,0.20] : (s'=5) +
    [0.03,0.25] : (s'=6) +
    [0.03,0.25] : (s'=7) +
    [0.03,0.25] : (s'=8);

  // --- action b ---
  // lo(goal)=5*0.03=0.15; hi(sink)=3*0.15=0.45 → Pmin(goal) ≥ max(0.15, 0.55)=0.55
  [b] s=0 ->
    [0.03,0.22] : (s'=1) +
    [0.03,0.22] : (s'=2) +
    [0.03,0.22] : (s'=3) +
    [0.03,0.22] : (s'=4) +
    [0.03,0.22] : (s'=5) +
    [0.02,0.15] : (s'=6) +
    [0.02,0.15] : (s'=7) +
    [0.02,0.15] : (s'=8);

  // Route successors
  [t1] s=1 -> (s'=9);
  [t1] s=2 -> (s'=9);
  [t1] s=3 -> (s'=9);
  [t1] s=4 -> (s'=9);
  [t1] s=5 -> (s'=9);
  [t2] s=6 -> (s'=10);
  [t2] s=7 -> (s'=10);
  [t2] s=8 -> (s'=10);

  // Absorbing
  [end1] s=9  -> (s'=9);
  [end2] s=10 -> (s'=10);
endmodule

rewards
  [a] true : 1;
  [b] true : 1;
endrewards

label "goal" = s=9;
label "Crash" = s=10;
